{
  "name": "leafy",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Bookmarker",
  "icons": {
    "16": "icons/leafy16.png",
    "48": "icons/leafy48.png",
    "128": "icons/leafy128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/js/vendor/jquery.min.js",
      "src/js/vendor/jquery.cookie.js",
      "src/js/vendor/underscore.min.js",
      "src/js/vendor/backbone.min.js",
      "src/js/leafy.js",
      "src/js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "cookies",
    "contextMenus",
    "http://localhost/*",
    "http://useleafy.com/*"
  ],
  "browser_action": {
    "default_icon": "icons/leafy19.png",
    "default_title": "Save to Leafy"
  }
}
