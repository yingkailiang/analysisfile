<h1>Listing cookies</h1>

<table>
  <tr>
    <th>Data</th>
    <th>Zombie</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cookies.each do |cookie| %>
  <tr>
    <td><%= cookie.data %></td>
    <td><%= cookie.zombie %></td>
    <td><%= link_to 'Show', cookie %></td>
    <td><%= link_to 'Destroy', cookie, :data => {confirm: 'Are you sure?'}, method: :delete %></td>
  </tr>
<% end %>
</table>

<br />


